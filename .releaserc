{
  "branches": ["+([0-9])?(.{+([0-9]),x}).x", "master", "next", "next-major", {"name": "beta", "prerelease": true}, {"name": "alpha", "prerelease": true}],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/npm",
    ["@semantic-release/git", {
      "assets": ["package.json", "CHANGELOG.md", "obsidian-manager/styles.css", "obsidian-manager/main.js", "obsidian-manager/manifest.json"],
      "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }],
    ["@semantic-release/github", {
      "assets": [
        {"path": "package.json", "label": "package.json"},
        {"path": "CHANGELOG.md", "label": "CHANGELOG.md"},
        {"path": "obsidian-manager/styles.css", "label": "styles.css"},
        {"path": "obsidian-manager/main.js", "label": "main.js"},
        {"path": "obsidian-manager/manifest.json", "label": "manifest.json"}
      ]
    }]
  ]
}
